#include <iostream>
#include <vector>
#include <algorithm>
#define MAX_LENGTH 20


using namespace std;
int main(void){
    vector<char> v;
    char s1[MAX_LENGTH];

    printf("문자열 s1을 입력하시오");
    scanf("%s",s1);
    printf("s1: %s\n",s1);

    for(int i=0; i<strlen.(s1);i++;){
    v.push_back(s1[i]);}
    sort(v.begin(),v.end());
    v.erase(unique(v.begin(),v.end()),v.end());
    for(int i=0; i<v.size();i++){
        cout<<v[i]<<' ';
    }
    return 0;
}
